{
  "name": "eslint-repl-web",
  "version": "0.1.0",
  "homepage": "https://bradzacher.github.io/papercraft-printer",
  "private": true,
  "dependencies": {
    "@types/react": "^16.7.6",
    "react": "^16.7.0-alpha",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.7.0-alpha",
    "react-jss": "^8.6.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint src typings config scripts --ext=.js,.ts,.tsx",
    "format-no-write": "prettier-eslint src/**/*.{ts,tsx} typings/**/*.{ts,tsx} --eslint-config-path=.eslintrc --eslint-ignore --prettier-ignore --eslint-path=node_modules/eslint --config=.prettierrc",
    "format": "yarn format-no-write --write",
    "format-check": "yarn format-no-write --list-different"
  },
