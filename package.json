    "raf": "3.4.0",
    "react-dev-utils": "^5.0.2",
    "react-dropzone": "^7.0.1",
    "resolve": "1.6.0",
    "source-map-loader": "^0.2.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
